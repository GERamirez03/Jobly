{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Chief Technical Officer"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                500000
            ]
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.50
            ]
        },
        "companyHandle": {
            "type": "string",
            "default": "",
            "title": "The companyHandle Schema",
            "examples": [
                "hall-davis"
            ]
        }
    },
    "examples": [{
        "title": "Chief Technical Officer",
        "salary": 500000,
        "equity": 0.50,
        "companyHandle": "hall-davis"
    }]
}